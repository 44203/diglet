{
  "name": "diglet",
  "version": "2.0.0-beta1",
  "description": "secure end-to-end encrypted reverse https tunnel",
  "main": "index.js",
  "bin": {
    "diglet": "bin/diglet.js",
    "diglet-server": "bin/diglet-server.js",
    "diglet-tunnel": "bin/diglet-tunnel.js"
  },
  "scripts": {
    "test": "mocha --exit",
    "linter": "eslint index.js lib test",
    "coverage": "istanbul cover _mocha -- --exit",
    "release-tunnel": "pkg ./bin/diglet-tunnel.js --out-path dist",
    "release-server": "pkg ./bin/diglet-server.js --out-path dist",
    "release": "npm run release-tunnel && npm run release-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/bookchin/diglet.git"
  },
  "keywords": [
    "tunnel",
    "https"
  ],
  "author": "Gordon Hall <gordon@gordonwritescode.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://gitlab.com/bookchin/diglet/issues"
  },
  "homepage": "https://gitlab.com/bookchin/diglet#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^5.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "pkg": "^4.3.3",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6",
    "ws": "^1.1.1"
  },
  "dependencies": {
    "async": "^2.6.1",
    "bunyan": "^1.8.5",
    "commander": "^2.9.0",
    "merge": "^1.2.0",
    "rc": "^1.1.6",
    "secp256k1": "^3.5.0",
    "tldjs": "^1.7.0"
  }
}
